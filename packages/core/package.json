{
	"name": "@meng-xi/uni-router",
	"type": "module",
	"version": "0.1.4",
	"private": false,
	"description": "为uni-app量身定制的路由插件。其提供了与uni-app无缝集成的特性，使得路由管理变得简单而高效。",
	"readme": "README.md",
	"author": {
		"name": "PedroQue99",
		"email": "ksmengfang@163.com"
	},
	"license": "MIT",
	"homepage": "https://github.com/MengXi-Studio/uni-router#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MengXi-Studio/uni-router.git"
	},
	"bugs": "https://github.com/MengXi-Studio/uni-router/issues",
	"keywords": [
		"uni-app",
		"router",
		"uni-router",
		"plugin",
		"navigation",
		"routing"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"build": "unbuild",
		"stub": "unbuild --stub",
		"prepublishOnly": "pnpm build"
	},
	"peerDependencies": {
		"vite": "^5.0.0"
	},
	"dependencies": {
		"globby": "^14.0.0",
		"json5": "^2.2.3",
		"unconfig": "^7.3.2",
		"yaml": "^2.8.0"
	},
	"devDependencies": {
		"@dcloudio/types": "^3.4.16",
		"@types/node": "^24.1.0",
		"vite": "^5.4.1"
	}
}
